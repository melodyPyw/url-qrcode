{
	"manifest_version": 2,
	"name": "url-qrcode",
	"version": "0.0.1",
	"description": "通过URL生成二维码的插件",
	"icons":
	{
		"16": "img/icon.png",
		"48": "img/icon.png",
		"128": "img/icon.png"
	},
	"background":
	{
		"page": "background.html"
	},
	"browser_action": 
	{
		"default_icon": "img/icon.png",
		"default_title": "通过URL生成二维码的插件",
		"default_popup": "popup.html"
	},
	"content_scripts": 
	[
		{
			"matches": ["<all_urls>"],
			"js": ["js/content-script.js"],
			"run_at": "document_start"
		}
	],
    "default_locale": "zh_CN"
}